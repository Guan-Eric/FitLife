{
  "cli": {
    "version": "14.1.0"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "withoutGoogleServices": true,
      "env": {
        "FIREBASE_API_KEY": "@FIREBASE_API_KEY",
        "ADMOB_IOS_APP_ID": "@ADMOB_IOS_APP_ID",
        "ADMOB_ANDROID_APP_ID": "@ADMOB_ANDROID_APP_ID",
        "ADMOB_FEED_UNIT_ID": "@ADMOB_FEED_UNIT_ID",
        "ADMOB_STREAK_UNIT_ID_IOS": "@ADMOB_STREAK_UNIT_ID_IOS"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "withoutGoogleServices": true,
      "env": {
        "FIREBASE_API_KEY": "@FIREBASE_API_KEY",
        "ADMOB_IOS_APP_ID": "@ADMOB_IOS_APP_ID",
        "ADMOB_ANDROID_APP_ID": "@ADMOB_ANDROID_APP_ID",
        "ADMOB_FEED_UNIT_ID": "@ADMOB_FEED_UNIT_ID",
        "ADMOB_STREAK_UNIT_ID_IOS": "@ADMOB_STREAK_UNIT_ID_IOS"
      }
    },
    "production": {
      "environment": "production",
      "withoutGoogleServices": true,
      "env": {
        "FIREBASE_API_KEY": "@FIREBASE_API_KEY",
        "ADMOB_IOS_APP_ID": "@ADMOB_IOS_APP_ID",
        "ADMOB_ANDROID_APP_ID": "@ADMOB_ANDROID_APP_ID",
        "ADMOB_FEED_UNIT_ID": "@ADMOB_FEED_UNIT_ID",
        "ADMOB_STREAK_UNIT_ID_IOS": "@ADMOB_STREAK_UNIT_ID_IOS"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
